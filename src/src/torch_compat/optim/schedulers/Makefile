# -----------------------------------------------------------------
# Dependencies - - - - - - - - - - - - - - - - - - - - - - - - - - 
# -----------------------------------------------------------------
ROOT_PATH := ../../../..
include $(ROOT_PATH)/Makefile.config
# -----------------------------------------------------------------
# Local Makefile Variables - - - - - - - - - - - - - - - - - - - - 
# -----------------------------------------------------------------
HERE_PATH := $(SRC_PATH)/torch_compat/optim/schedulers
COMPILE_CMD = $(LOG_START) && $(GXX) $(GXX_FLAGS) $(USUAL_INCLUDE_PATHS) $(TORCH_INCLUDE_PATHS) -c $< -o $@ && $(LOG_END)
# -----------------------------------------------------------------
# - - - Pytorch's learning schedules implemented in Libtorch - - - 
# -----------------------------------------------------------------
SCHEDULERS := $(OUTPUT_PATH)/lambda_lr_scheduler.o
# -----------------------------------------------------------------
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
# -----------------------------------------------------------------
$(OUTPUT_PATH)/lambda_lr_scheduler.o: $(HERE_PATH)/lambda_lr_scheduler.cpp $(USUALS_DEPS)
	@$(COMPILE_CMD)
.PHONY: schedulers
schedulers: $(SCHEDULERS)
	@$(LOG_SUCCESS)
# -----------------------------------------------------------------
# - - - General Target -- - - - - - - - - - - - - - - - - - - - - -
# -----------------------------------------------------------------
.PHONY: all
all: schedulers
	@$(LOG_SUCCESS)
