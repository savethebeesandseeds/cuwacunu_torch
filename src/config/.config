[GENERAL]
    exchange_type                  = TEST
    absolute_base_symbol           = USDT
    iinuji_logs_buffer_capacity    = 6000   # max in-memory entries shown in F8 logs
    torch_seed                     = 42
    board_contract_config_filename = /cuwacunu/src/config/default.board.contract.config
    hashimyei_store_root           = /cuwacunu/data/hashimyei
    hashimyei_metadata_secret      = CHANGE_ME_HASHIMYEI_METADATA_SECRET

[DATA_LOADER]
    # Batch size now comes from tsiemene wave DSL: WAVE_PROFILE.BATCH_SIZE
    dataloader_workers             = 0       # deterministic default (set >0 only when prioritizing throughput)
    dataloader_force_rebuild_cache = true    # rebuild raw/normalized dataset cache artifacts when stale/changed
    dataloader_range_warn_batches  = 256     # unbounded range-mode emits warning meta when estimated batches exceed this threshold
    dataloader_csv_bootstrap_deltas = 64     # number of positive deltas sampled to infer regular lattice step
    dataloader_csv_step_abs_tol    = 1e-8    # absolute tolerance for step-ratio near-integer validation
    dataloader_csv_step_rel_tol    = 1e-10   # relative tolerance for step-ratio near-integer validation

[REAL_EXCHANGE]
    Ed25519_pkey                   = /cuwacunu/src/config/secrets/real/ed25519key.pem
    EXCHANGE_api_filename          = /cuwacunu/src/config/secrets/real/exchange.key
    websocket_url                  = wss://ws-api.binance.com:443/ws-api/v3

[TEST_EXCHANGE]
    Ed25519_pkey                   = /cuwacunu/src/config/secrets/test/ed25519key.pem
    EXCHANGE_api_filename          = /cuwacunu/src/config/secrets/test/exchange.key
    websocket_url                  = wss://testnet.binance.vision/ws-api/v3
